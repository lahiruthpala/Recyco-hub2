h1 {
    line-height: 1.35;
}

.button,
.button .material-icons,
canvas,
img,
video {
    vertical-align: middle;
}

:root {
    --main-color: #3498db;
    /* --light-gray: #444; */
    --light-gray: white;
    --smoke-color: #666;
}

.text-color--smoke {
    color: var(--smoke-color);
}

.color--light-blue {
    background-color: #03a9f4 !important;
}

.layout-spacer {
    flex-grow: 1;
}

.color--green {
    background-color: #00d45a !important;
}

html {
    color: rgba(255, 255, 255, .87);
}

::selection {
    background: #b3d4fc;
    text-shadow: none;
}

body,
html {
    width: 100%;
}

@media print {

    img,
    tr {
        page-break-inside: avoid;
    }

    *,
    :after,
    :before,
    :first-letter {
        background: 0 0 !important;
        color: #000 !important;
        box-shadow: none !important;
    }

    a,
    a:visited {
        text-decoration: underline;
    }

    a[href]:after {
        content: " ("attr(href) ")";
    }

    thead {
        display: table-header-group;
    }

    img {
        max-width: 100% !important;
    }

    h2 {
        orphans: 3;
        widows: 3;
    }

    h2 {
        page-break-after: avoid;
    }
}

.button,
.card,
a {
    font-size: 16px;
    font-weight: 400;
    color: black;
    text-decoration: none;
}

a:hover {
    color: green;
}

.linkinfo {
    font-size: 16px;
    font-weight: 300;
    text-decoration: none;
}

.linkinfo:hover {
    color: green;
}

html {
    height: 100%;
    -ms-touch-action: manipulation;
    touch-action: manipulation;
}

body {
    min-height: 100%;
    margin: 0;
}

main {
    display: block;
}

[hidden] {
    display: none !important;
}

body,
html {
    font-family: Helvetica, Arial, sans-serif;
    font-size: 14px;
    font-weight: 400;
    line-height: 20px;
}

h1,
h2,
h6 {
    margin: 0;
    padding: 0;
}

h1,
h2 {
    margin-bottom: 24px;
    font-weight: 400;
}

h1,
h2,
h6 {
    margin-top: 24px;
    font-family: Roboto, Helvetica, Arial, sans-serif;
}

h1 {
    font-size: 56px;
    letter-spacing: -.02em;
}

h2 {
    font-size: 45px;
    line-height: 48px;
}

h6 {
    font-weight: inherit;
    margin-bottom: 16px;
    line-height: 24px;
}

h6 {
    font-size: 16px;
    letter-spacing: .04em;
}

ul {
    font-weight: 400;
    letter-spacing: 0;
}

ul {
    font-size: 14px;
    line-height: 24px;
}

.material-icons {
    font-family: 'Material Icons';
    font-weight: 400;
    font-style: normal;
    font-size: 24px;
    line-height: 1;
    letter-spacing: normal;
    text-transform: none;
    display: inline-block;
    word-wrap: normal;
    font-feature-settings: 'liga';
    -webkit-font-feature-settings: 'liga';
    -webkit-font-smoothing: antialiased;
}

.badge[data-badge]:after,
.button {
    font-family: Roboto, Helvetica, Arial, sans-serif;
}

.ripple {
    background: #000;
    border-radius: 50%;
    height: 50px;
    left: 0;
    opacity: 0;
    pointer-events: none;
    position: absolute;
    top: 0;
    transform: translate(-50%, -50%);
    width: 50px;
    overflow: hidden;
}

.badge {
    position: relative;
    white-space: nowrap;
    margin-right: 24px;
}

.badge[data-badge]:after {
    content: attr(data-badge);
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: center;
    align-content: center;
    align-items: center;
    position: absolute;
    top: -11px;
    right: -24px;
    font-weight: 600;
    font-size: 12px;
    width: 22px;
    height: 22px;
    border-radius: 50%;
    background: #00bcd4;
    color: #fff;
}

.badge.badge--overlap {
    margin-right: 10px;
}

.badge.badge--overlap:after {
    right: -10px;
}

.button {
    background: 0 0;
    border: none;
    border-radius: 2px;
    color: #fff;
    position: relative;
    height: 36px;
    margin: 0;
    min-width: 64px;
    padding: 0 16px;
    display: inline-block;
    font-size: 14px;
    letter-spacing: 0;
    overflow: hidden;
    will-change: box-shadow;
    transition: box-shadow .2s cubic-bezier(.4, 0, 1, 1), background-color .2s cubic-bezier(.4, 0, .2, 1), color .2s cubic-bezier(.4, 0, .2, 1);
    outline: 0;
    cursor: pointer;
    text-decoration: none;
    text-align: center;
    line-height: 36px;
}

.button--icon {
    font-size: 24px;
    padding: 0;
    overflow: hidden;
}

.data-table tbody tr {
    transition-duration: .28s;
    transition-timing-function: cubic-bezier(.4, 0, .2, 1);
}

.button:hover {
    background-color: rgba(158, 158, 158, .2);
}

.button:focus:not(:active) {
    background-color: rgba(0, 0, 0, .12);
}

.button:active {
    background-color: rgba(158, 158, 158, .4);
}

.button--raised {
    background: rgba(158, 158, 158, .2);
    /* box-shadow: 0 2px 2px 0 rgba(0, 0, 0, .14), 0 3px 1px -2px rgba(0, 0, 0, .2), 0 1px 5px 0 rgba(0, 0, 0, .12); */
}

.button--raised:active {
    box-shadow: 0 4px 5px 0 rgba(0, 0, 0, .14), 0 1px 10px 0 rgba(0, 0, 0, .12), 0 2px 4px -1px rgba(0, 0, 0, .2);
    background-color: rgba(158, 158, 158, .4);
}

.button--raised:focus:not(:active) {
    box-shadow: 0 0 8px rgba(0, 0, 0, .18), 0 8px 16px rgba(0, 0, 0, .36);
    background-color: rgba(158, 158, 158, .4);
}

.button--icon {
    border-radius: 50%;
    height: 32px;
    margin-left: 0;
    margin-right: 0;
    min-width: 32px;
    width: 32px;
    color: inherit;
    line-height: normal;
}

.button--icon .material-icons {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-12px, -12px);
    line-height: 24px;
    width: 24px;
}

.button__ripple-container {
    display: block;
    height: 100%;
    left: 0;
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 0;
    overflow: hidden;
}

.card {
    display: flex;
    flex-direction: column;
    font-size: 16px;
    font-weight: 400;
    min-height: auto;
    width: auto;
    z-index: 1;
    position: relative;
    background: white;
    border-radius: 20px;
    box-sizing: border-box;
}

.info {
    display: flex;
    flex-direction: column;
    font-size: 16px;
    font-weight: 400;
    min-height: auto;
    width: auto;
    z-index: 1;
    position: relative;
    background: white;
    border-radius: 0 0 15px 15px;
    box-sizing: border-box;
    border: 2px solid #268252;
    width: 100%;
    color: #000;
}

.card__title {
    align-items: center;
    color: rgba(255, 255, 255, 1);
    display: block;
    display: flex;
    justify-content: stretch;
    line-height: normal;
    padding: 20px 16px;
    perspective-origin: 165px 56px;
    transform-origin: 165px 56px;
    box-sizing: border-box;
    border-radius: 15px 15px 0 0;
    /* margin: 0 16px 0 16px; */
    display: flex;
    flex-direction: column;
    border: solid 0 0 2px 0;
    border-bottom-width: 1px;
    border-bottom-style: solid;
    border-bottom-color: #ccc;
}

.card__title-text {
    align-self: flex-start;
    color: inherit;
    display: block;
    display: flex;
    font-size: 20px;
    font-weight: 400;
    line-height: normal;
    overflow: hidden;
    transform-origin: 149px 48px;
    margin: 0;
    align-self: center;
}

.card__supporting-text {
    color: rgba(255, 255, 255, 1);
    font-size: 1rem;
    padding: 20px 16px;
    /* background-image: linear-gradient(to bottom right, white, #8BD2A0); */
    border-radius: 0 0 15px 15px;
}

.data-table {
    position: relative;
    border-collapse: collapse;
    white-space: nowrap;
    font-size: 16px;
    border-radius: 15px;
}

.data-table thead {
    padding-bottom: 3px;
}

.data-table tbody tr {
    position: relative;
    height: 48px;
    transition-property: background-color;
}

.data-table tbody tr:hover {
    background-color: rgba(0, 0, 0, .2);
}

.data-table td,
.data-table th {
    padding: 0 8px 12px;
    text-align: left;
}

.data-table td {
    position: relative;
    vertical-align: middle;
    height: 48px;
    border: 0.5px solid rgba(255, 255, 255, .5);
    padding-top: 12px;
    box-sizing: border-box;
    color: black;
}

.data-table th {
    position: relative;
    vertical-align: bottom;
    text-overflow: ellipsis;
    font-weight: 700;
    line-height: 24px;
    letter-spacing: 0;
    height: 48px;
    font-size: 13px;
    color: #fff;
    padding-bottom: 8px;
    box-sizing: border-box;
    /* Add this line to round the corners of the th elements */
}

.data-table th:first-child {
    border-radius: 15px 0 0 0;
}

.data-table th:last-child {
    border-radius: 0 15px 0 0;
}

table.data-table tr:nth-child(odd) {
    background-color: white;
}


table.data-table tr:nth-child(even) {
    background-color: #F3F3F3;
}

.data-table__cell--header {
    background-color: #16896E;
}

.label--mini {
    vertical-align: middle;
}

.data-table__cell--non-numeric.data-table__cell--non-numeric {
    text-align: left;
}

.layout__header {
    box-shadow: 0 2px 2px 0 rgba(0, 0, 0, .14), 0 3px 1px -2px rgba(0, 0, 0, .2), 0 1px 5px 0 rgba(0, 0, 0, .12);
}

.icon-toggle__label {
    display: inline-block;
    position: relative;
    cursor: pointer;
    height: 32px;
    width: 32px;
    min-width: 32px;
    color: #616161;
    border-radius: 50%;
    padding: 0;
    margin-left: 0;
    margin-right: 0;
    text-align: center;
    background-color: transparent;
    will-change: background-color;
    transition: background-color .2s cubic-bezier(.4, 0, .2, 1), color .2s cubic-bezier(.4, 0, .2, 1);
}

.icon-toggle__label.material-icons {
    line-height: 32px;
    font-size: 24px;
}

.list {
    display: block;
    padding: 0;
    list-style: none;
}

.list__item {
    font-size: 16px;
    font-weight: 400;
    letter-spacing: .04em;
    line-height: 1;
    display: flex;
    min-height: 48px;
    flex-direction: row;
    flex-wrap: nowrap;
    align-items: center;
    padding: 16px;
    cursor: default;
    color: #fff;
    overflow: hidden;
}

.list__item .list__item-primary-content {
    order: 0;
    flex-grow: 2;
    text-decoration: none;
    box-sizing: border-box;
    display: flex;
    align-items: center;
}

.list__item .list__item-primary-content .list__item-avatar,
.list__item .list__item-primary-content .list__item-icon {
    margin-right: 16px;
}

.list__item .list__item-secondary-content {
    display: flex;
    flex-flow: column;
    align-items: flex-end;
    margin-left: 16px;
}

.list__item-icon,
.list__item-icon.material-icons {
    height: 24px;
    width: 24px;
    font-size: 24px;
    box-sizing: border-box;
    color: black;
}

.list__item-avatar,
.list__item-avatar.material-icons {
    height: 32px;
    width: 32px;
    box-sizing: border-box;
    border-radius: 50%;
    background-color: rgba(255, 255, 255, .8);
    font-size: 32px;
    color: #fff;
}

.menu__container,
.menu__outline {
    padding: 0;
    border: none;
    position: absolute;
    display: block;
}

.menu__item {
    font-size: 14px;
    letter-spacing: 0;
    font-weight: 400;
}

.list__item--two-line {
    height: 64px;
}

.list__item--two-line .list__item-primary-content {
    height: 32px;
    line-height: 20px;
    display: block;
}

.list__item--two-line .list__item-primary-content .list__item-avatar {
    float: left;
}

.list__item--two-line .list__item-primary-content .list__item-sub-title {
    font-size: 14px;
    font-weight: 400;
    letter-spacing: 0;
    line-height: 18px;
    color: rgba(255, 255, 255, .54);
    display: block;
    padding: 0;
}

.menu__container {
    margin: 0;
    overflow: visible;
    height: 0;
    width: 0;
    visibility: hidden;
    z-index: -1;
}

.menu__container.is-visible {
    z-index: 999;
    visibility: visible;
}

.menu__outline {
    background: #EDEDED;
    margin: 0;
    border-radius: 15px;
    border: 1px solid green;
    top: 0;
    left: 0;
    overflow: hidden;
    opacity: 0;
    transform: scale(0);
    transform-origin: 0 0;
    will-change: transform;
    transition: transform .3s cubic-bezier(.4, 0, .2, 1), opacity .2s cubic-bezier(.4, 0, .2, 1);
    z-index: -1;
}

.menu__item {
    background-color: transparent;
}

.menu__container.is-visible .menu__outline {
    opacity: 1;
    transform: scale(1);
    z-index: 999;
}

.menu__outline.menu--bottom-right {
    transform-origin: 100% 0;
}

.menu {
    position: absolute;
    list-style: none;
    top: 0;
    left: 0;
    height: auto;
    width: auto;
    min-width: 124px;
    padding: 8px 0;
    margin: 0;
    opacity: 0;
    clip: rect(0 0 0 0);
    z-index: -1;
}

.menu__container.is-visible .menu {
    opacity: 1;
    z-index: 999;
}

.menu.menu--bottom-right {
    left: auto;
    right: 0;
}

.menu__item {
    display: block;
    border: none;
    color: rgba(0, 0, 0, .87);
    text-align: left;
    margin: 0;
    padding: 0 16px;
    outline-color: #bdbdbd;
    position: relative;
    overflow: hidden;
    text-decoration: none;
    cursor: pointer;
    height: 48px;
    line-height: 48px;
    white-space: nowrap;
    opacity: 0;
    transition: opacity .2s cubic-bezier(.4, 0, .2, 1);
    user-select: none;
}

.menu__item:focus {
    outline: 0;
}

.menu__container.is-visible .menu__item {
    opacity: 1;
}

.menu__item:hover {
    background-color: rgba(0, 0, 0, .2);
}

.menu__item:active,
.menu__item:focus {
    background-color: rgba(0, 0, 0, .15);
}

.layout__header {
    transition-duration: .2s;
    transition-timing-function: cubic-bezier(.4, 0, .2, 1);
}

.layout {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    overflow-y: auto;
    overflow-x: hidden;
    position: relative;
    -webkit-overflow-scrolling: touch;
}

.layout__container {
    position: absolute;
    width: 100%;
    height: 100%;
}

.layout__header {
    display: flex;
    flex-direction: column;
    flex-wrap: nowrap;
    justify-content: flex-start;
    box-sizing: border-box;
    flex-shrink: 0;
    margin: 0;
    padding: 0;
    border: none;
    min-height: 64px;
    max-height: 1000px;
    z-index: 3;
    background-color: white;
    color: #00bcd4;
    transition-property: max-height, box-shadow;
}

@media screen and (max-width:1440px) {
    .layout__header {
        min-height: 56px;
        display: none;
    }
}

.layout__header-row {
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    flex-shrink: 0;
    box-sizing: border-box;
    align-self: stretch;
    align-items: center;
    height: 64px;
    margin: 0;
    padding: 0 5% 0 5%;
}

@media screen and (max-width:1440px) {
    .layout__header-row {
        height: 56px;
        padding: 0 8px 0 72px;
    }
}

.layout__header-row>* {
    flex-shrink: 0;
}

.layout__content {
    -ms-flex: 0 1 auto;
    position: relative;
    display: inline-block;
    overflow-y: auto;
    overflow-x: hidden;
    flex-grow: 1;
    z-index: 1;
    padding: 0 50px;
    -webkit-overflow-scrolling: touch;
}

.layout--fixed-drawer>.layout__content {
    margin-left: 0;
}

@media screen and (max-width:1440px) {
    .layout--fixed-drawer>.layout__content {
        margin-left: 0;
    }
}

.textfield {
    position: relative;
    font-size: 16px;
    display: inline-block;
    box-sizing: border-box;
    max-width: 100%;
    margin: 0;
    padding: 20px 0;
}

.textfield .button {
    position: absolute;
    bottom: 20px;
}

.textfield--expandable {
    min-width: 32px;
    width: auto;
    min-height: 32px;
}

.textfield__input,
.textfield__label {
    font-size: 16px;
    width: 100%;
    text-align: left;
    display: block;
}

.textfield--expandable .button--icon {
    top: 16px;
}

.textfield__input {
    border: none;
    border-bottom: 1px solid rgba(255, 255, 255, .2);
    font-family: Helvetica, Arial, sans-serif;
    margin: 0;
    padding: 4px 0;
    background: 0 0;
    color: inherit;
}

a {
    font-family: Roboto, Helvetica, Arial, sans-serif;
}

.textfield__label {
    bottom: 0;
    color: rgba(0, 0, 0, .26);
    left: 0;
    right: 0;
    pointer-events: none;
    position: absolute;
    top: 24px;
    overflow: hidden;
    white-space: nowrap;
}

.textfield__expandable-holder,
.textfield__label:after {
    transition-duration: .2s;
    transition-timing-function: cubic-bezier(.4, 0, .2, 1);
}

.textfield__label:after {
    background-color: #00bcd4;
    bottom: 20px;
    content: '';
    height: 2px;
    left: 45%;
    position: absolute;
    visibility: hidden;
    width: 10px;
}

.textfield__expandable-holder {
    position: relative;
    margin-left: 32px;
    display: inline-block;
    max-width: .1px;
}

.textfield__expandable-holder .textfield__label:after {
    bottom: 0;
}

.shadow--2dp {
    box-shadow: 0 2px 2px 0 rgba(0, 0, 0, .14), 0 3px 1px -2px rgba(0, 0, 0, .2), 0 1px 5px 0 rgba(0, 0, 0, .12);
}

.grid {
    display: flex;
    flex-flow: row wrap;
    margin: 0 auto;
    align-items: stretch;
}

.grid.grid--no-spacing {
    padding: 0;
}

.cell {
    box-sizing: border-box;
}

.cell--top {
    align-self: flex-start;
}

.grid.grid--no-spacing>.cell {
    margin: 0;
}

@media (max-width:1151px) {
    .grid {
        padding: 0;
    }

    .cell {
        margin: 16px;
        width: calc(100% - 32px);
    }

    .grid--no-spacing>.cell {
        width: 100%;
    }

    .cell--2-col-phone.cell--2-col-phone {
        width: calc(50% - 32px);
    }

    .cell--4-col-phone.cell--4-col-phone {
        width: calc(100% - 32px);
    }

    .grid--no-spacing>.cell--4-col-phone.cell--4-col-phone {
        width: 100%;
    }
}

@media (min-width:1152px) and (max-width:1919px) {
    .grid {
        padding: 0;
    }

    .cell {
        margin: 16px;
        width: calc(33.33333% - 32px);
    }

    .grid--no-spacing>.cell {
        width: 33.33333%;
    }

    .cell--4-col-tablet.cell--4-col-tablet {
        width: calc(33.33333% - 32px);
    }

    .cell--12-col-tablet.cell--12-col-tablet {
        width: calc(100% - 32px);
    }

    .grid--no-spacing>.cell--12-col-tablet.cell--12-col-tablet {
        width: 100%;
    }
}

@media (min-width:1920px) {
    .grid {
        padding: 0;
    }

    .cell {
        margin: 16px;
        width: calc(33.33333% - 32px);
    }

    .grid--no-spacing>.cell {
        width: 33.33333%;
    }

    .cell--4-col-desktop.cell--4-col-desktop {
        width: calc(33.33333% - 32px);
    }

    .cell--9-col-desktop.cell--9-col-desktop {
        width: calc(75% - 32px);
    }

    .grid--no-spacing>.cell--9-col-desktop.cell--9-col-desktop {
        width: 75%;
    }

    .cell--12-col-desktop.cell--12-col-desktop {
        width: calc(100% - 32px);
    }
}

.layout {
    background: #202020;
    overflow: hidden;
}

main {
    background-size: cover;
    background-color: #EDEDED
        /* linear-gradient(to bottom right, white, #8BD2A0) */
}

.layout__header {
    width: 100% !important;
    margin-left: 0 !important;
}

.layout__header-row .avatar-dropdown:hover,
.layout__header-row .material-icons.badge:hover,
.layout__header-row .button--icon:hover {
    background: rgba(0, 0, 0, .3);
}

.layout__header-row {
    color: black;
}

.layout__header-row .avatar-dropdown {
    cursor: pointer;
    margin: 0 10px;
    padding: 0 20px;
    line-height: 64px;
}

.layout__header-row .avatar-dropdown>img {
    width: 32px;
    height: 32px;
}

.layout__header-row .avatar-dropdown>span {
    color: black;
    padding-right: 16px;
}

.layout__header-row .textfield {
    padding: 18px 0;
}

.layout__header-row .material-icons.badge {
    margin: 10px;
    cursor: pointer;
    overflow: visible;
    text-align: center;
    height: 24px;
